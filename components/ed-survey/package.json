{
  "name": "ed-survey",
  "description": "A webcomponent for surveys in education.",
  "version": "0.0.0",
  "author": "Benjamin Abel <dev.abel@free.fr> (lyceum.fr)",
  "keywords": [
    "survey",
    "web-component"
  ],
  "type": "module",
  "customElements": "custom-elements.json",
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@open-wc/testing": "^3.1.6",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/jest": "^29.5.3",
    "@web/dev-server-esbuild": "^0.4.1",
    "@web/dev-server-rollup": "^0.5.2",
    "@web/test-runner": "^0.17.0",
    "tslib": "^2.3.1",
    "typescript": "^5.1.6"
  },
  "exports": {
    ".": "./dist/index.js",
    "./ed-survey.js": "./dist/ed-survey.js"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "analyze": "cem analyze --globs \"src/Ed*.ts\"",
    "build": "tsc && npm run analyze",
    "prepublish": "tsc && npm run analyze",
    "start": "tsc && ../../node_modules/.bin/concurrently -k \"tsc --watch --preserveWatchOutput\" \"npm run analyze -- --watch\"",
    "test": "cd .. && npm run test -- --group $npm_package_name",
    "test:watch": "cd .. && npm run test -- --watch --group $npm_package_name"
  }
}